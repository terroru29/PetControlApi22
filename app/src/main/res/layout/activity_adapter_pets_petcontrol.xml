<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdapterPetsPetControl">

    <ImageButton
        android:id="@+id/imbPets"
        android:layout_width="93dp"
        android:layout_height="88dp"
        android:layout_marginStart="24dp"
        android:background="@drawable/circle_button"
        android:contentDescription="@string/description_imageButton"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.127"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.083" />

    <TextView
        android:id="@+id/txtNamesPets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:gravity="center"
        android:textColor="@color/black"
        android:typeface="serif"
        android:textStyle="bold"
        android:textSize="15sp"
        android:autoSizeTextType="uniform"
        android:visibility="gone"
        android:textAllCaps="true"
        app:layout_constraintBottom_toBottomOf="@+id/imbPets"
        app:layout_constraintEnd_toEndOf="@+id/imbPets"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/imbPets"
        app:layout_constraintTop_toTopOf="@+id/imbPets"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="UnusedAttribute"
        tools:targetApi="o"/>
</androidx.constraintlayout.widget.ConstraintLayout>